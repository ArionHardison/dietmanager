{
  "name": "dietmanager",
  "version": "1.0.0",
  "description": "DietManager - Full-stack AI Diet Management Utility",
  "private": true,
  "workspaces": [
    "dietmanager-client"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev:api\" \"npm run dev:client\"",
    "dev:api": "node dev-server.mjs",
    "dev:client": "cd dietmanager-client && API_URL=http://localhost:4000 npm run dev",
    "dev:vercel": "vercel dev",
    "build": "cd dietmanager-client && npm run build",
    "start": "npm run start:client",
    "start:client": "cd dietmanager-client && npm run start",
    "install:all": "npm install && cd dietmanager-client && npm install",
    "vercel": "vercel",
    "vercel:prod": "vercel --prod",
    "lint": "cd dietmanager-client && npm run lint",
    "clean": "rm -rf node_modules dietmanager-client/node_modules dietmanager-client/.nuxt dietmanager-client/dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dietmanager/dietmanager.git"
  },
  "keywords": [
    "diet",
    "nutrition",
    "ai",
    "meal-planning",
    "health",
    "fitness",
    "calorie-tracking",
    "nuxt",
    "vue",
    "vercel"
  ],
  "author": "DietManager Team",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^7.6.0",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "vercel": "^28.0.0"
  },
  "engines": {
    "node": "20.x",
    "npm": ">=6.0.0"
  }
}
